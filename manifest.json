{
    "app_id": "com.mattermost.servicenow",
    "app_type": "aws_lambda",
    "version": "0.2.0",
    "display_name": "ServiceNow",
    "description": "ServiceNow integration",
    "homepage_url": "https://github.com/mattermost/mattermost-app-servicenow",
    "root_url": "",
    "icon": "now-mobile-icon.png",
    "requested_permissions": [
        "act_as_bot",
        "remote_oauth2",
        "act_as_user"
    ],
    "requested_locations": [
        "/command",
        "/channel_header",
        "/post_menu"
    ],
    "aws_lambda": [
        {
            "path": "/",
            "name": "go-function",
            "handler": "mattermost-app-servicenow",
            "runtime": "go1.x"
        }
    ],
    "get_oauth2_connect_url": {
        "path": "/oauth2/connect",
        "expand": {
            "oauth2_app": "all"
        }
    },
    "on_oauth2_complete": {
        "path": "/oauth2/complete",
        "expand": {
            "oauth2_app": "all",
            "acting_user_access_token": "all"
        }
    },
    "bindings": {
        "path": "/bindings",
        "expand": {
            "acting_user": "all",
            "oauth2_user": "all"
        }
    },
    "assets": []
}
