{
    "app_id": "servicenow",
    "version": "0.2.0",
    "display_name": "ServiceNow",
    "description": "ServiceNow integration",
    "homepage_url": "https://github.com/mattermost/mattermost-app-servicenow",
    "icon": "icon.png",
    "requested_permissions": [
        "act_as_bot",
        "remote_oauth2",
        "act_as_user"
    ],
    "requested_locations": [
        "/command",
        "/channel_header",
        "/post_menu"
    ],
    "aws_lambda": {
        "functions": [
            {
                "path": "/",
                "name": "go-function",
                "handler": "mattermost-app-servicenow",
                "runtime": "go1.x"
            }
        ]
    },
    "http": {
        "root_url": "http://localhost:4445"
    },
    "plugin": {
        "plugin_id": "com.mattermost.app.servicenow"
    },
    "get_oauth2_connect_url": {
        "path": "/oauth2/connect",
        "expand": {
            "oauth2_app": "all"
        }
    },
    "on_oauth2_complete": {
        "path": "/oauth2/complete",
        "expand": {
            "oauth2_app": "all",
            "acting_user_access_token": "all"
        }
    },
    "bindings": {
        "path": "/bindings",
        "expand": {
            "acting_user": "all",
            "oauth2_app": "all",
            "oauth2_user": "all"
        }
    },
    "assets": []
}