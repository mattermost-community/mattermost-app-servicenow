{
    "app_id": "servicenow",
    "version": "0.3.3",
    "display_name": "ServiceNow (deprecated)",
    "description": "Do not install. This integration is for existing users only. Check back soon because a terrific new ServiceNow Integration is on it's way!",
    "homepage_url": "https://github.com/mattermost/mattermost-app-servicenow",
    "icon": "icon.png",
    "requested_permissions": [
        "act_as_bot",
        "remote_oauth2",
        "act_as_user"
    ],
    "requested_locations": [
        "/command",
        "/channel_header",
        "/post_menu"
    ],
    "app_type": "aws_lambda",
    "aws_lambda": [
        {
            "path": "/",
            "name": "servicenow",
            "handler": "servicenow",
            "runtime": "go1.x"
        }
    ],
    "get_oauth2_connect_url": {
        "path": "/oauth2/connect",
        "expand": {
            "oauth2_app": "all"
        }
    },
    "on_oauth2_complete": {
        "path": "/oauth2/complete",
        "expand": {
            "oauth2_app": "all",
            "acting_user_access_token": "all"
        }
    },
    "bindings": {
        "path": "/bindings",
        "expand": {
            "acting_user": "all",
            "oauth2_app": "all",
            "oauth2_user": "all"
        }
    },
    "assets": []
}